{
  "manifest_version": 3,
  "name": "PokeClicker Fill Queue Button",
  "version": "1.0",
  "description": "Add a button to fill the breeding queue in PokeClicker.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://www.pokeclicker.com/"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
